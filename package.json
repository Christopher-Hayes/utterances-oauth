{
  "name": "utterances-oauth",
  "version": "0.1.0",
  "description": "GitHub OAuth server for utterances",
  "main": "dist/index.js",
  "scripts": {
    "start": "cfworker run --watch --inspect src/index.ts",
    "build": "cfworker build src/index.ts",
    "deploy": "cfworker deploy --name comment-on-hayes-software --route 'comment.hayes.software/*' src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christopher-hayes/utterances-oauth"
  },
  "license": "MIT",
  "devDependencies": {
    "@cfworker/dev": "^1.14.2",
    "@cloudflare/workers-types": "^4.20240806.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {}
}
